- extends "form_base.hamlpy"

- block form
    %form{:method => "post"}
        %h2.form-heading
            New Horse Show
        {{form.as_p}}
        {% csrf_token %}
        %input.btn{:name => "submit", :type => "submit", :value => "Create Show"}
    :javascript
        $(document).ready(function () {
            $('.datepicker-default:first').datepicker({
                format:'mm/dd/yyyy'
            });
            $('.timepicker-default:last').timepicker();
        });
        